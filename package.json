{
  "name": "solid-auth-service",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node --stack-trace-limit=40 dist/app.js",
    "build": "tsc --project tsconfig.json",
    "dev": "npm run build && npm start",
    "redis-start": "docker start authorization-agent || docker run -d --name authorization-agent -p 6379:6379 redis/redis-stack-server:latest",
    "redis-stop": "docker start authorization-agent",
    "redis-remove": "docker rm -f authorization-agent",
    "redis-remove-image": "docker rmi -f redis/redis-stack-server:latest",
    "redis-cli": "docker exec -it authorization-agent redis-cli",
    "test": "jest",
    "check": "prettier --check src && eslint src test",
    "fmt": "prettier --write src && eslint src test",
    "server-run": "npx @solid/community-server -c @css:config/file.json -f solid-server/"
  },
  "dependencies": {
    "@inrupt/solid-client": "^1.30.2",
    "@inrupt/solid-client-authn-node": "^1.17.4",
    "@rdfjs/types": "^1.1.0",
    "@solid/access-token-verifier": "^2.0.5",
    "@types/http-link-header": "^1.0.5",
    "@types/jest": "^29.5.10",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "cookie-parser": "~1.4.4",
    "cookie-session": "^2.0.0",
    "debug": "~2.6.9",
    "dotenv": "^16.3.1",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "express": "~4.16.1",
    "ioredis": "^5.3.2",
    "jest": "^29.7.0",
    "morgan": "~1.9.1",
    "n3": "^1.17.2",
    "prettier": "3.0.3",
    "solid-interoperability": "file:../solid-interoperability",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.6",
    "@types/cookie-session": "^2.0.47",
    "@types/debug": "^4.1.11",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/n3": "^1.16.4"
  }
}
